{
    "name": "Recipe Extension",
    "description": "A simple way to get your recipes",
    "version": "1.0",
    "manifest_version": 3,
    "background": {
        "service_worker": "background.js"
    },
    "permissions": ["activeTab", "scripting"],
    "action": {
        "default_popup": "background.html",
        "default_icon": {
            "42": "/images/cookie.png"
        }
    },
    "icons": {

    }
}